{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "ordered-imports": true,
        "object-literal-sort-keys": false,
        "no-console": {
            "severity": "warning"
        },
        "variable-name": {
            "options": "allow-leading-underscore"
        }
    }
}